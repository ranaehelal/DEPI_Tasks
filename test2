public class Main2
{
    //Cassete c =new Cassete ("old");
    Player play =new play ();
    player.play("old");
    
}

interface Player{
    void play (String oldTape);
}
class Cassete implements Player{
    void play(String oldTape) {
        System.out.print(oldTape);
    }
}


class Mp3{
    void playMp3(String newtape){
        System.out.print(newtape);
    }
}

class Adapter implements Player{
    Mp3 mp3;
    void play(String oldTape) {
        mp3.playMp3(oldTape);
    }
    
}



